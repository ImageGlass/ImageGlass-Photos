{
	"name": "imageglass-photos",
	"version": "0.0.1",
	"description": "A lightweight, versatile photo viewer",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build": "set NODE_ENV=dev & webpack --progress",
		"dev": "set NODE_ENV=dev & webpack --watch --progress",
		"toolkit": "electron ./node_modules/electron-toolkit"
	},
	"repository": "https://github.com/imageglass/imageglass-photos",
	"keywords": [
		"ImageGlass Photos",
		"fast",
		"image",
		"viewer",
		"open source"
	],
	"author": "Duong Dieu Phap",
	"license": "MIT",
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"devtron": "^1.4.0",
		"electron": "~1.7.8",
		"electron-builder": "^19.49.0",
		"electron-toolkit": "^1.0.24"
	},
	"dependencies": {
		"css-loader": "^0.28.7",
		"extract-text-webpack-plugin": "^3.0.2",
		"jquery": "^3.2.1",
		"node-sass": "^4.7.2",
		"sass-loader": "^6.0.6",
		"webpack": "^3.10.0"
	},
	"build": {
		"appId": "com.electron.imageglass-photos",
		"copyright": "Copyright Â© 2018 Duong Dieu Phap",
		"productName": "imageglass-photos",
		"electronVersion": "1.7.9",
		"asar": true,
		"directories": {
			"app": "./",
			"output": "dist"
		},
		"win": {
			"target": [
				{
					"target": "AppX (Windows Store)",
					"arch": [
						"x64"
					]
				}
			]
		}
	}
}